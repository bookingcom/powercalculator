    "babel-jest": "^26.0.1",
    "cryptiles": "^4.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.0.1",
    "jstat": "^1.9.3",
    "rands": "^1.0.0",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.38.3",
